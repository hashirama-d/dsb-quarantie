<div class="page">

  <header role="banner" class="header">
    <div class="container">
      {{ page.header }}

    </div>
    {% if page.highlighted %}
    <div class="banner">
      {{ page.highlighted }}
    </div>
    {% endif %}
  </header>

  {{ page.primary_menu }}



  <div class="main-content">
    <main role="main">
      <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

          {{ page.content }}

    </main>
  </div>

  <footer role="contentinfo" class="footer">
    <div class="container">
      {{ page.footer }}
    </div>
  </footer>

</div>{# /.page #}
